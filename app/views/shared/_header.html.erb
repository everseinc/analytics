<header class="header">
	<div class="header-title">Everse</div>
	<button class="header-menu-button" id="header-menu-button">
    <i class="material-icons">more_vert</i>
  </button>
</header>
<div id='header-menu' class="menu">
	<ul>
		<li><%= link_to  "新規ユーザー登録", new_customer_path %></li>
		<% if logged_in? %>
		<li><%= link_to "マイページ", customer_path(current_customer.id) %></li>
		<li><%= link_to "Log out", logout_path, method: "delete" %></li>
		<% else %>
		<li><%= link_to "Log in", login_path %></li>
		<% end %>
	</ul>
</div>
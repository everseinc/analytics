<div id="ratio_graph">

	<% @report.dim_stores.each do |dim_store| %>	
	<div class="ratio_graph_card mdl-card mdl-shadow--2dp">
	  <div class="mdl-card__title mdl-card--expand">
	    <h4><%= dim_store.key %></h4>
	  </div>
	  <div class="ratio_graph_card_text mdl-card__supporting-text">
	  	<div class="ratio_graph_field">
				<canvas class="ratio_graph" data-id="<%= dim_store[:id] %>"></canvas>
			</div>
	  </div>
    <div class="mdl-card__actions mdl-card--border">
    	<%= link_to "MORE", dimension_path(dim_store.id), :class => "mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %>
	  </div>
	</div>
	<% end %>
</div>
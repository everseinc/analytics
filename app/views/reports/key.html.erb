<div id="report_main">

	<!-- filer view -->
	<%= render partial: "reports/key/view_option" %>

	<!-- arrow view -->
	<div class="key_report_arrow">
		<i class="material-icons key_report_arrow_icon">arrow_downward</i>
	</div>

	<!-- loading indicator -->
	<div id="key_report_loader" class="key_report_loader">
		<div id="p2" class="mdl-progress mdl-js-progress mdl-progress__indeterminate"></div>
	</div>

	<!-- main view -->
	<div class="key_report_main">

		<!-- info about blocks -->
		<h1 class="report_contents_title">block情報</h1>
		<%= render partial: "reports/key/info" %>

		<% @report.emotions.each do |emotion| %>
			<h1 class="report_contents_title report_contents_title_emotion"><%= emotion.name %></h1>
			<%= render partial: "reports/key/emotion_card", locals: {emotion: emotion}%>
		<% end %>
	</div>
</div>

<!-- ui controller_button -->
<%= render partial: "reports/shared/controller_button" %>